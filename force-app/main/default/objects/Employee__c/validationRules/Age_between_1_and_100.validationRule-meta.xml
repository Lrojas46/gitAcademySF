<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_between_1_and_100</fullName>
    <active>true</active>
    <errorConditionFormula>Age__c &lt; 0  ||  Age__c &gt; 100</errorConditionFormula>
    <errorMessage>Age is not valid, please enter a valid age</errorMessage>
</ValidationRule>
